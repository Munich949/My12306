<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dlnu.index12306.biz.userservice.dao.mapper.UserPhoneMapper">

    <!-- 注销用户 -->
    <update id="deletionUser" parameterType="UserPhoneDO">
        UPDATE t_user_phone
        SET deletion_time = #{deletionTime},
            del_flag      = '1'
        WHERE phone = #{phone}
          AND del_flag = '0'
    </update>
</mapper>